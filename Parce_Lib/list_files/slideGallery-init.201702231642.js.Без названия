{for(var i=0;i<elements.length;i++)
{var min=0;var max=elements[i].getElement('.carousel').getElements('li').length-1;var random=elements[i].getElement('.carousel').getAttribute('data-no_random')?false:true;if(isNaN(max))max=0;var small_gallery=new slideGallery(elements[i],{current:0,steps:8,mode:"line",random:random,stop:".stop",start:".start",duration:7000,speed:400,nextItem:'.next-carousel',prevItem:'.prev-carousel'});}}
